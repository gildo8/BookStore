@model IEnumerable<BookStore.Models.List>

<div class="listTitle">Search - Results</div>
        @foreach (var item in Model)
        {
            <br />
            <table id="bookList" style="width:100%">
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Year)</td>
                    <td class="title">@Html.DisplayFor(modelItem => item.Title) / @Html.DisplayFor(modelItem => item.Author)</td>
                    <td class="detailsList">
                        <center>
                            @Html.ActionLink("Details", "Details", new { id = item.ListID }) @if (User.Identity.Name.Equals("Admin@gmail.com"))
                            {@Html.Raw(" / ") @Html.ActionLink("Edit", "Edit", new { id = item.ListID })}
                        </center>
                    </td>
                </tr>
                <tr>
                    <td></td>
                    <td class="content"><p>@Html.DisplayFor(modelItem => item.Content)</p></td>
                    <td><img src="@Html.DisplayFor(modelItem => item.Picture)" style="width :100px" alt=" picture" /></td>
                </tr>
                <tr>
                    <td></td>
                    <td class="publisher">@Html.DisplayFor(modelItem => item.Publisher) / @Html.DisplayFor(modelItem => item.Genre)</td>
                    <td class="price">@Html.DisplayFor(modelItem => item.Price)</td>
                </tr>
            </table>
        }
    <br />
    <center><div class="back">@Html.ActionLink("Search Again", "SearchBook", "List")</div></center>

